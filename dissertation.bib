@article{ZakiItTakesTwo2008,
  langid = {english},
  title = {It {{Takes Two The Interpersonal Nature}} of {{Empathic Accuracy}}},
  volume = {19},
  issn = {0956-7976, 1467-9280},
  url = {http://pss.sagepub.com/content/19/4/399},
  doi = {10.1111/j.1467-9280.2008.02099.x},
  abstract = {Although current theories suggest that affective empathy (perceivers' experience of social targets' emotions) should contribute to empathic accuracy (perceivers' ability to accurately assess targets' emotions), extant research has failed to consistently demonstrate a correspondence between them. We reasoned that prior null findings may be attributable to a failure to account for the fundamentally interpersonal nature of empathy, and tested the prediction that empathic accuracy may depend on both targets' tendency to express emotion and perceivers' tendency to empathically share that emotion. Using a continuous affect-rating paradigm, we found that perceivers' trait affective empathy was unrelated to empathic accuracy unless targets' trait expressivity was taken into account: Perceivers' trait affective empathy predicted accuracy only for expressive targets. These data suggest that perceivers' self-reported affective empathy can indeed predict their empathic accuracy, but only when targets' expressivity allows their thoughts and feelings to be read.},
  number = {4},
  journaltitle = {Psychological Science},
  shortjournal = {Psychological Science},
  urldate = {2013-03-18},
  date = {2008-04-01},
  pages = {399-404},
  author = {Zaki, Jamil and Bolger, Niall and Ochsner, Kevin},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/AVPJ7RIB/Zaki et al. - 2008 - It Takes Two The Interpersonal Nature of Empathic .pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/SZZBIC32/399.html}
}

@article{KilnerMoreonepathway2011,
  title = {More than One Pathway to Action Understanding},
  volume = {15},
  issn = {1364-6613},
  url = {http://www.sciencedirect.com/science/article/pii/S1364661311001100},
  doi = {10.1016/j.tics.2011.06.005},
  abstract = {Many believe that the ability to understand the actions of others is made possible by mirror neurons and a network of brain areas known as the action-observation network (AON). Despite nearly two decades of research into mirror neurons and the AON, however, there is little evidence that they enable the inference of the intention of observed actions. Instead, theories of action selection during action execution indicate that a ventral pathway, linking middle temporal gyrus with the anterior inferior frontal gyrus, might encode these abstract features during action observation. Here I propose that action understanding requires more than merely the AON, and might be achieved through interactions between a ventral pathway and the dorsal AON.},
  number = {8},
  journaltitle = {Trends in Cognitive Sciences},
  shortjournal = {Trends in Cognitive Sciences},
  urldate = {2015-01-13},
  date = {2011-08},
  pages = {352-357},
  author = {Kilner, James M.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2D2TVUKF/Kilner - 2011 - More than one pathway to action understanding.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WIJ2ABHP/Kilner - 2011 - More than one pathway to action understanding.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/BI9E33S9/S1364661311001100.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/HM9M3SS9/S1364661311001100.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WJIKDMFQ/S1364661311001100.html}
