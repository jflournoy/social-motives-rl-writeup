  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/PIK7R8A8/Nelson and Crick - 1999 - Rose-Colored Glasses Examining the Social Informa.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WVI547TB/17.html}
}

