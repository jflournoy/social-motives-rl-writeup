@incollection{HarterSelf2007,
  langid = {english},
  title = {The {{Self}}},
  isbn = {978-0-470-14765-8},
  url = {http://onlinelibrary.wiley.com/doi/10.1002/9780470147658.chpsy0309/abstract},
  abstract = {Interest in self-processes has recently burgeoned in many branches of psychology, for example, cognitive development, personality and social psychology, clinical psychology, memory development, to name the major fields. In this chapter, historical perspectives on the self are first presented, including the contributions of William James and Charles Horton Cooley, over a century ago. A major focus is on developmental change in the self. Three periods of childhood (early, middle, and late) and three periods of adolescence (early, middle, and late) are delineated. Cognitive, social, and affective processes responsible for changes in the self are then identified. In addition, normative liabilities associated with movement to new stages are discussed. Issues involving more maladaptive self-processes are presented, along with clinical implications. The topic of the stability of self-evaluations is then discussed where it is argued, for example, that global self-esteem can change to the extent that its causes or antecedents change. Divisive arguments that it is either intractably stable or subject to change are dismissed as too simplistic and not supported by evidence. Finally, gender, ethnic, and cross-cultural factors impacting the self are discussed to broaden our approaches to an understanding of the origins of self-evaluation.},
  booktitle = {Handbook of {{Child Psychology}}},
  publisher = {{John Wiley \& Sons, Inc.}},
  urldate = {2015-11-29},
  date = {2007},
  keywords = {domain-specific self-concepts,false self-behavior,multiple selves,self-worth,depression,self-esteem},
  author = {Harter, Susan},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/Q8JUADH9/Harter_2006_Self.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E9EHR7UT/abstract.html}
}

