  doi = {10.1016/j.psyneuen.2006.03.001},
  abstract = {Summary
The dexamethasone suppression test (DST), frequently abnormal in mood disorder patients, is considered to measure glucocorticoid receptor-mediated negative feedback. We examined the hypothesis that the, apparently more sensitive, dexamethasone/corticotrophin-releasing-hormone (dex/CRH) test unveils subtle hypothalamicâ€“pituitaryâ€“adrenal axis disturbance not detected by the DST in 82 patients with mood disorders and 28 controls. There was a close correlation between the cortisol responses on the two tests (rs=0.73, p\&lt;0.0005). However, ROC analysis revealed that the dex/CRH test had better diagnostic performance than the DST (p=0.031). The sensitivity of delta cortisol (from the dex/CRH) was 61.9\% and the specificity 71.4\%. The sensitivity of 1500\&\#xa0;h cortisol (the DST) was 66.6\% and the specificity was 47.6\%. This suggests that the two tests measure common pathology but that the dex/CRH test has better diagnostic utility.},
  number = {7},
  journaltitle = {Psychoneuroendocrinology},
  urldate = {2011-10-01},
  date = {2006-08},
  pages = {889-894},
  keywords = {Adrenal cortex hormones,Corticotrophin releasing hormone,Cortisol,DST,Dexamethasone,depression,dex/CRH test},
  author = {Watson, Stuart and Gallagher, Peter and Smith, Margaret S. and Ferrier, I. Nicol and Young, Allan H.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/I2EXNZ3V/Watson et al. - 2006 - The dexCRH testâ€”Is it better than the DST.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/A8QV8SZ7/S0306453006000503.html}
}

