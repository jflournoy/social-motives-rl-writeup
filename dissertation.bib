  author = {Chase, Paul A. and Hilliard, Lacey J. and Geldhof, G. John and Warren, Daniel J. A. and Lerner, Richard M.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/U52ADZIR/Chase et al. - 2014 - Academic Achievement in the High School Years The.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4QEJCG8T/s10964-013-0085-4.html}
