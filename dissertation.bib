@article{anokhin2010,
  langid = {english},
  title = {Heritability of {{Delay Discounting}} in {{Adolescence}}: {{A Longitudinal Twin Study}}},
  volume = {41},
  issn = {0001-8244, 1573-3297},
  url = {http://link.springer.com.libproxy.uoregon.edu/article/10.1007/s10519-010-9384-7},
  doi = {10.1007/s10519-010-9384-7},
  shorttitle = {Heritability of {{Delay Discounting}} in {{Adolescence}}},
  abstract = {Delay discounting (DD) refers to the preference for smaller immediate rewards over larger but delayed rewards, and is considered to be a distinct component of a broader “impulsivity” construct. Although greater propensity for discounting the value of delayed gratification has been associated with a range of problem behaviors and substance abuse, particularly in adolescents, the origins of individual differences in DD remain unclear. We examined genetic and environmental influences on a real-life behavioral measure of DD using a longitudinal twin design. Adolescent participants were asked to choose between a smaller (\$7) reward available immediately and a larger (\$10) reward to be received in 7 days. Biometrical genetic analysis using linear structural equation modeling showed significant heritability of DD at ages 12 and 14 (30 and 51\%, respectively) and suggested that the same genetic factors influenced the trait at both ages. DD was significantly associated with symptoms of conduct disorder, attention deficit hyperactivity disorder, substance use, and with higher novelty seeking and poor self-regulation. This study provides the first evidence for heritability of DD in humans and suggests that DD can be a promising endophenotype for genetic studies of addiction and externalizing disorders.},
  number = {2},
  journaltitle = {Behavior Genetics},
  shortjournal = {Behav Genet},
  urldate = {2015-10-29},
  date = {2010-08-11},
  pages = {175-183},
  keywords = {Clinical Psychology,Decision Making,Delay discounting,Health Psychology,Impulsivity,Public Health/Gesundheitswesen,adolescents,Heritability},
  author = {Anokhin, Andrey P. and Golosheykin, Simon and Grant, Julia D. and Heath, Andrew C.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/R8ABHZMK/s10519-010-9384-7.html}
}

@article{steinberg2009,
