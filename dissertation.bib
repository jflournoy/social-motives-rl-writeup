  author = {Gorgolewski, Krzysztof J. and Varoquaux, Gael and Rivera, Gabriel and Schwartz, Yannick and Sochat, Vanessa V. and Ghosh, Satrajit S. and Maumet, Camille and Nichols, Thomas E. and Poline, Jean-Baptiste and Yarkoni, Tal and Margulies, Daniel S. and Poldrack, Russell A.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4PCG5T7H/Gorgolewski et al. - 2016 - NeuroVault.org A repository for sharing unthresho.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/IA73HUAV/S1053811915003067.html}
}

