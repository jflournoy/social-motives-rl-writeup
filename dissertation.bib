  author = {Warner, Lori J. and Lumley, Mark A. and Casey, Rita J. and Pierantoni, Wayne and Salazar, Reina and Zoratti, Edward M. and Enberg, Robert and Simon, Michael R.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/D3NWKP3X/Warner et al. - 2006 - Health Effects of Written Emotional Disclosure in .pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ERZHF6BA/557.html},
