@article{LaBouffDifferencesAttitudesOutgroups2012,
  title = {Differences in {{Attitudes Toward Outgroups}} in {{Religious}} and {{Nonreligious Contexts}} in a {{Multinational Sample}}: {{A Situational Context Priming Study}}},
