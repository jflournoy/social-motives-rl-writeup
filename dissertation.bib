  pages = {893-909},
  keywords = {Situations,person–situation transactions,personality state,situation experience,situation characteristic,Big Six HEXACO,Situational Eight DIAMONDS,cross-lagged models},
  author = {Rauthmann, John F. and Jones, Ashley Bell and Sherman, Ryne A.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ICJ2UHQQ/893.html},
