  author = {Critchley, Hugo D. and Wiens, Stefan and Rotshtein, Pia and Öhman, Arne and Dolan, Raymond J.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/66XA7XTV/Critchley et al. - 2004 - Neural systems supporting interoceptive awareness.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/G6NRW26A/Critchley et al. - 2004 - Neural systems supporting interoceptive awareness.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/H4V9E5IS/nn1176.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/V883TXBC/nn1176.html}
