@article{strauss2009,
  langid = {english},
  title = {Construct {{Validity}}: {{Advances}} in {{Theory}} and {{Methodology}}},
  volume = {5},
  issn = {1548-5943, 1548-5951},
  url = {http://www.annualreviews.org/doi/10.1146/annurev.clinpsy.032408.153639},
  doi = {10.1146/annurev.clinpsy.032408.153639},
  shorttitle = {Construct {{Validity}}},
  abstract = {Measures of psychological constructs are validated by testing whether they relate to measures of other constructs as speciﬁed by theory. Each test of relations between measures reﬂects on the validity of both the measures and the theory driving the test. Construct validation concerns the simultaneous process of measure and theory validation. In this article, we review the recent history of validation efforts in clinical psychological science that has led to this perspective, and we review the following recent advances in validation theory and methodology of importance for clinical researchers. These are: the emergence of nonjustiﬁcationist philosophy of science; an increasing appreciation for theory and the need for informative tests of construct validity; valid construct representation in experimental psychopathology; the need to avoid representing multidimensional constructs with a single score; and the emergence of effective new statistical tools for the evaluation of convergent and discriminant validity.},
  number = {1},
  journaltitle = {Annual Review of Clinical Psychology},
  urldate = {2018-05-14},
  date = {2009-04},
  pages = {1-25},
  author = {Strauss, Milton E. and Smith, Gregory T.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E6AH7MDE/strauss smith 2009 AnnRevClinPsy - construct validity advances.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/LWTWQ8EB/Strauss and Smith - 2009 - Construct Validity Advances in Theory and Methodo.pdf}
