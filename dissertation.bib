  keywords = {Adolescent,Anemia; Iron-Deficiency,Asperger Syndrome,Autistic Disorder,Child,Child; Preschool,Comorbidity,Cross-Sectional Studies,Female,Ferritins,Hemoglobinometry,Humans,Infant,Male,Prognosis,Wales},
