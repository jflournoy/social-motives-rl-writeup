  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/HQXS9VZK/Schilbach et al. - 2012 - Introspective Minds Using ALE Meta-Analyses to St.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/VP4TTBHK/Schilbach et al. - 2012 - Introspective Minds Using ALE Meta-Analyses to St.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/NTNSK42C/infodoi10.1371journal.pone.html}
