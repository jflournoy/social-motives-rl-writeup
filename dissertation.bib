  keywords = {Bayesian,Dual process,Hierarchical models,Recognition memory,Signal detection},
  author = {Pratte, Michael S. and Rouder, Jeffrey N.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ASSHA2IH/S0022249610001082.html}
