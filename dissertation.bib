  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GIWWPEZV/Soliman et al. - 2015 - It’s Not “All in Your Head” Understanding Religion.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/C8X6BMBE/852.html},
  eprinttype = {pmid},
  eprint = {26581739}
