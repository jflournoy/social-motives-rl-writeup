  keywords = {Brain development,Neurodevelopmental MRI Database,Lifespan MRI,Average MRI templates},
  author = {Richards, John E. and Sanchez, Carmen and Phillips-Meek, Michelle and Xie, Wanze},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/HTUS8F3X/Richards et al. - 2016 - A database of age-appropriate average MRI template.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4NM4KSJK/S1053811915003559.html}
