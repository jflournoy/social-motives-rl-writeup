  pages = {80-98},
  keywords = {child abuse and neglect,child mental health,child welfare,evidence-based practice,implementation research},
  author = {Landsverk, John and Garland, Ann and Reutz, Jennifer Rolls and Davis, Inger},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/Z3A5DKNQ/Landsverk et al. - 2011 - Bridging science and practice in child welfare and.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JDFV8U7H/80.html}
