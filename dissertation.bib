@article{lakens,
  langid = {english},
  title = {Improving {{Inferences}} about {{Null Effects}} with {{Bayes Factors}} and {{Equivalence Tests}}},
  url = {https://academic.oup.com/psychsocgerontology/advance-article/doi/10.1093/geronb/gby065/5033832},
  doi = {10.1093/geronb/gby065},
  abstract = {Abstract.  Researchers often conclude an effect is absent when a null-hypothesis significance test yields a non-significant p-value. However, it is neither logi},
