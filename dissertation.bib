@article{Juruenaprednisolonesuppressiontest2010,
  title = {The Prednisolone Suppression Test in Depression: {{Doseâ}}€“response and Changes with Antidepressant Treatment},
  volume = {35},
  issn = {0306-4530},
  url = {http://www.sciencedirect.com/science/article/pii/S0306453010001186},
  doi = {10.1016/j.psyneuen.2010.04.016},
  shorttitle = {The Prednisolone Suppression Test in Depression},
  abstract = {Summary
Depressed patients have reduced glucocorticoid receptor (GR) function, as demonstrated by resistance to the suppressive effects of the synthetic glucocorticoid hormone, and GR agonist, dexamethasone. We have developed a suppressive test with prednisolone, a synthetic glucocorticoid that is similar to cortisol in its pharmacodynamics and pharmacokinetics, and binds to both the GR and the mineralocorticoid receptor (MR). We have found that depressed patients suppress normally to prednisolone, unless they are particularly non-responsive to treatment. In the present study, we evaluated 28 inpatients with treatment-resistant depression (TRD), and compared salivary cortisol secretion (at 0900\&\#xa0;h, 1200\&\#xa0;h and 1700\&\#xa0;h) after placebo or after prednisolone (5\&\#xa0;mg), before and after an inpatient treatment admission. Half of the patients (n\&\#xa0;=\&\#xa0;14) reached treatment response. When comparing the assessment between admission and discharge, cortisol output after placebo fell (âˆ’26\% of area under the curve; p\&\#xa0;=\&\#xa0;0.024) while the output after prednisolone did not change. Moreover, there was no change in the response to prednisolone (percentage suppression) between admission at discharge, and this was not influenced by treatment response. Finally, we could confirm and extend our previously published data with prednisolone (5\&\#xa0;mg), showing that depressed patients (n\&\#xa0;=\&\#xa0;12) and controls (n\&\#xa0;=\&\#xa0;12) suppressed equally to both 5 and 10\&\#xa0;mg doses of prednisolone. This study suggests that the response to prednisolone is similar in depressed patients and controls at different doses of prednisolone, and does not change with symptomatic improvement. This is in contrast with findings, from us and others, using other measures of hypothalamicâ€“pituitaryâ€“adrenal axis function, such as basal cortisol levels or the response to dexamethasone. Thus, we propose that the prednisolone suppression test may offer specific biological and clinical information, related to its action at both the GR and the MR.},
  number = {10},
  journaltitle = {Psychoneuroendocrinology},
  urldate = {2011-10-08},
  date = {2010-11},
  pages = {1486-1491},
  keywords = {Cortisol,Hypothalamicâ€“pituitaryâ€“adrenal (HPA) axis,Negative feedback,Prednisolone,Treatment-resistant depression,depression,glucocorticoid receptor,mineralocorticoid receptor},
  author = {Juruena, Mario F. and Cleare, Anthony J. and Papadopoulos, Andrew S. and Poon, Lucia and Lightman, Stafford and Pariante, Carmine M.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/B37GT8RR/Juruena et al. - 2010 - The prednisolone suppression test in depression D.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/9QHNQJIB/S0306453010001186.html}
}

@article{CarterLivingvirtualcommunities2005,
  title = {Living in Virtual Communities: An Ethnography of Human Relationships in Cyberspace},
  volume = {8},
  issn = {1369-118X},
  url = {http://dx.doi.org/10.1080/13691180500146235},
  doi = {10.1080/13691180500146235},
  shorttitle = {Living in Virtual Communities},
  abstract = {This paper outlines some of the issues involved in the development of human relationships in cyberspace. Set within the wider context of the Internet and society it investigates how geographically distant individuals are coming together on the Internet to inhabit new kinds of social spaces or virtual communities. People ‘live in’ and ‘construct’ these new spaces in such a way as to suggest that the Internet is not a placeless cyberspace that is distinct and separate from the real world. Building on the work of other cyberethnographers, the author combines original ethnographic research in Cybercity, a Virtual Community, with face-to-face meetings to illustrate how, for many people, cyberspace is just another place to meet. Second, she suggests that people in Cybercity are investing as much effort in maintaining relationships in cyberspace as in other social spaces. Her preliminary analysis suggests that by extending traditional human relationships into Cybercity, they are widening their webs of relationships, not weakening them. Human relationships in cyberspace are formed and maintained in similar ways to those in wider society. Rather than being exotic and removed from real life, they are actually being assimilated into everyday life. Furthermore, they are often moved into other social settings, just as they are in offline life.},
  number = {2},
  journaltitle = {Information, Communication \& Society},
  urldate = {2014-10-07},
  date = {2005-06-01},
  pages = {148-167},
  author = {Carter, Denise},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4A6J4XN6/13691180500146235.html}
}

