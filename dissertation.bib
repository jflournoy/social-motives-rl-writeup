  date = {2004-01-09},
  pages = {1095-1107},
  keywords = {belonging,empathic accuracy,interpersonal sensitivity,rejection},
  author = {Pickett, Cynthia L. and Gardner, Wendi L. and Knowles, Megan},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FIGIEZTZ/Pickett et al. - 2004 - Getting a Cue The Need to Belong and Enhanced Sen.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2KUZZDEA/1095.html}
}

