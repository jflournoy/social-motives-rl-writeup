  date = {2012-09-13},
  keywords = {Anxiety,Body,Interoception,Neuroticism,emotion},
  author = {Terasawa, Yuri and Shibata, Midori and Moriguchi, Yoshiya and Umeda, Satoshi},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/F5QWTN8B/Terasawa et al. - 2012 - Anterior insular cortex mediates bodily sensibilit.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/P6BX5TC8/Terasawa et al. - 2012 - Anterior insular cortex mediates bodily sensibilit.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/3BRK2D9F/scan.nss108.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/W72FI4MT/scan.nss108.html}
