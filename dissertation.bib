}

@article{2018,
  langid = {english},
  title = {Resilient Adolescents with a History of Adversity Have Normal Mood and Neural Responses to Social Rejection},
  url = {https://osf.io/tvcwd/},
  doi = {None},
  abstract = {Childhood adversity (CA) increases the risk of psychopathology. Yet adolescent social support (from family and/or friends) reduces the risk of psychopathology after CA. The resilience mechanisms of this effect remain unclear and we speculate that they are manifested by neurodevelopmental processes. Therefore, we investigated whether family and/or friendship support at age 14 and 17 mediate the relationship between CA before age 11 and affective or neural responses to social rejection at age 18. We studied 55 adolescents (26 with CA) with good mental health at age 18 (‘resilient’), from a longitudinal cohort. Participants underwent a Social Feedback Task in the MRI scanner. Social rejection feedback activated the dorsal Anterior Cingulate Cortex (dACC) and the left anterior Insula (AI). Family and friendship support did not mediate the relationship between CA and affective or neural (dACC \& AI) responses to social rejection. CA predicted better friendships at age 14, when adolescents with and without CA had comparable mood levels. CA did not predict affective or neural responses to social rejection. Thus, adolescents with CA who are characterized by mental health resilience at age 18 have better friendships at age 14, and normal affective and neural responses to social rejection at age 18. 
    Hosted on the Open Science Framework},
  journaltitle = {Open Science Framework},
  urldate = {2018-04-17},
  date = {2018-03-26},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JY7FQCKF/2018 - Resilient adolescents with a history of adversity .pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/D6MZWF6V/4rngb.html}
}

@article{devezer2018,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1803.10118},
  primaryClass = {stat},
  title = {Discovery of Truth Is Not Implied by Reproducibility but Facilitated by Innovation and Epistemic Diversity in a Model-Centric Framework},
  url = {http://arxiv.org/abs/1803.10118},
  abstract = {Theoretical work on reproducibility of scientific claims has hitherto focused on hypothesis testing. Focusing on hypothesis testing, however, poses a challenge to identify salient properties of the scientific process related to reproducibility, especially for fields that progress by building, comparing, selecting, and re-building models. We build a model-centric meta-scientific framework in which scientific discovery progresses by confirming models proposed in idealized experiments. In a temporal stochastic process of scientific discovery, we define scientists with diverse research strategies who search the true model generating the data. When there is no replication in the system, the structure of scientific discovery is a particularly simple Markov chain. We analyze the effect of diversity of research strategies in the scientific community and the complexity of the true model on the time spent at each model, the mean first time to hit the true model, and the rate of reproducibility given a true model. Inclusion of replication in the system breaks the Markov property and fundamentally alters the structure of scientific discovery. We analyze aforementioned properties of scientific discovery by an agent-based model. In our system, the seeming paradox of scientific progress despite irreproducibility persists even in the absence of questionable research practices and incentive structures, as the rate of reproducibility and scientific discovery of the truth are uncorrelated. We explain this seeming paradox by a combination of research strategies in the population and the state of truth. We find that innovation speeds up the discovery of truth by making otherwise inaccessible, possibly true models visible to the scientific population. We show that epistemic diversity in the scientific population optimizes across a range of desirable properties of scientific discovery.},
  urldate = {2018-04-17},
  date = {2018-03-27},
  keywords = {Statistics - Other Statistics},
  author = {Devezer, Berna and Nardin, Luis G. and Baumgaertner, Bert and Buzbas, Erkan},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/BEFVNIU4/Devezer et al. - 2018 - Discovery of truth is not implied by reproducibili.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/9HDQ5MI3/1803.html}
}

