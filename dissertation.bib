}

@article{vanGoethemSocialisingadolescentvolunteering2014,
  title = {Socialising Adolescent Volunteering: {{How}} Important Are Parents and Friends? {{Age}} Dependent Effects of Parents and Friends on Adolescents' Volunteering Behaviours},
