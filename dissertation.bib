  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/D78ZVBTJ/Galinsky et al. - 2015 - Maximizing the Gains and Minimizing the Pains of D.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E5KC7EZH/742.html},
