  date = {2013-10},
  pages = {113-124},
  keywords = {Adolescence,Nucleus accumbens,Reward,Striatum,development,fMRI},
  author = {Hoogendam, Janna Marie and Kahn, René S. and Hillegers, Manon H.J. and van Buuren, Mariët and Vink, Matthijs},
  options = {useprefix=true},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/PQBJEAWE/Hoogendam et al. - 2013 - Different developmental trajectories for anticipat.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/6QDWFXFR/S1878929313000510.html}
