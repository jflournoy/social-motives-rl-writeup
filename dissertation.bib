@article{foygel2011,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1112.5635},
  primaryClass = {math, stat},
  title = {Bayesian Model Choice and Information Criteria in Sparse Generalized Linear Models},
  url = {http://arxiv.org/abs/1112.5635},
  abstract = {We consider Bayesian model selection in generalized linear models that are high-dimensional, with the number of covariates p being large relative to the sample size n, but sparse in that the number of active covariates is small compared to p. Treating the covariates as random and adopting an asymptotic scenario in which p increases with n, we show that Bayesian model selection using certain priors on the set of models is asymptotically equivalent to selecting a model using an extended Bayesian information criterion. Moreover, we prove that the smallest true model is selected by either of these methods with probability tending to one. Having addressed random covariates, we are also able to give a consistency result for pseudo-likelihood approaches to high-dimensional sparse graphical modeling. Experiments on real data demonstrate good performance of the extended Bayesian information criterion for regression and for graphical models.},
  urldate = {2015-05-31},
  date = {2011-12-23},
  keywords = {Mathematics - Statistics Theory},
  author = {Foygel, Rina and Drton, Mathias},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/92AX653H/Foygel and Drton - 2011 - Bayesian model choice and information criteria in .pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WVGCW43J/1112.html}
}

@article{kammrath_pollyanna_2011,
  langid = {english},
  title = {The {{Pollyanna}} Myth: How Highly Agreeable People Judge Positive and Negative Relational Acts},
  volume = {37},
  issn = {1552-7433},
  doi = {10.1177/0146167211407641},
  shorttitle = {The {{Pollyanna}} Myth},
  abstract = {Although people high in agreeableness have often been shown to be positively biased toward others, four studies provide evidence that agreeableness is associated with extremity effects, not simple positivity effects, in social judgment. Across studies, agreeable participants judged prosocial behaviors more favorably, but antisocial behaviors more unfavorably, than did disagreeable participants. In support of a goal-congruence mechanism, Study 1 showed that communal goals, rather than perceived similarity, mediated the effects, and Studies 2-4 demonstrated that agreeable perceivers were particularly sensitive to communal (vs. agentic) violations. A longitudinal study of real-life impressions supported the laboratory evidence that agreeable people are highly sensitive to both the prosocial and antisocial behavior of others (Study 4). We discuss how the current account complements and extends existing theories of agreeableness.},
  number = {9},
  journaltitle = {Personality \& Social Psychology Bulletin},
  shortjournal = {Pers Soc Psychol Bull},
  date = {2011-09},
  pages = {1172-1184},
  keywords = {Adolescent,Female,Humans,Judgment,Longitudinal Studies,Male,Personality Inventory,Sex Factors,Social values,Young Adult,altruism,social perception,character,Antisocial Personality Disorder,Conflict (Psychology),Cooperative Behavior},
  author = {Kammrath, Lara K. and Scholer, Abigail A.},
  eprinttype = {pmid},
