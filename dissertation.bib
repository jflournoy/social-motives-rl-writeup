  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E35T4CVM/Laakmann et al. - 1985 - Dose-dependent growth hormone, prolactin and corti.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/RZ6C73NK/0306453085900423.html}
