  pages = {263--276},
  keywords = {Adolescent,HPA axis,early experience,parental care,temperament,_tablet},
  author = {Romer, Daniel},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/P24BQZXZ/Romer - 2010 - Adolescent risk taking, impulsivity, and brain dev.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/X3E8798P/Romer-2010-Adolescent_risk_taking,_impulsivity,_and_brain_development.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/DT6HGVW2/full.html}
