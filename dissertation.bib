  keywords = {Cognitive Psychology,Neurosciences},
  author = {Posner, Michael I. and Rothbart, Mary K. and Sheese, Brad E. and Tang, Yiyuan},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CKVHM3HI/Posner et al. - 2007 - The anterior cingulate gyrus and the mechanism of .pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ZAZJE6JZ/CABN.7.4.html}
}

