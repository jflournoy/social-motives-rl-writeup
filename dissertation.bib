  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2IUHVC8J/Tunney and Ziegler - 2015 - Toward a Psychology of Surrogate Decision Making.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/NKPKHTQP/880.html},
  eprinttype = {pmid},
  eprint = {26581742}
}

@article{BarnesPrioritizingSleepHealth2015,
