  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/QVKKWD53/Alpert et al. - 2016 - The Northwestern University Neuroimaging Data Arch.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/KZBPIDZB/S1053811915004504.html}
}

