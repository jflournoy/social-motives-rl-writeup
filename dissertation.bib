  keywords = {Metabolism,Neural Energetics,Response Time,fMRI,networks,neuroimaging},
  author = {Poldrack, Russell A.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CEDIKXG6/Poldrack - Is “efficiency” a useful concept in cognitive neur.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/BTA3EHRQ/S1878929314000413.html}
}

