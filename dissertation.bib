  date = {2015-11-01},
  pages = {721-726},
  keywords = {Academic achievement,sense of belonging,fixed-growth beliefs,mindsets},
  author = {Rattan, Aneeta and Savani, Krishna and Chugh, Dolly and Dweck, Carol S.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/KC4S46W7/Rattan et al. - 2015 - Leveraging Mindsets to Promote Academic Achievemen.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/D6H5S5XX/721.html},
