  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E45MT8VP/Ross and Schryer - 2015 - Outsourcing Memory in Response to an Aging Populat.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JDN7DICI/716.html},
  eprinttype = {pmid},
  eprint = {26581724}
}

@article{MannPromotingPublicHealth2015,
  langid = {english},
  title = {Promoting {{Public Health}} in the {{Context}} of the “{{Obesity Epidemic}}” {{False Starts}} and {{Promising New Directions}}},
