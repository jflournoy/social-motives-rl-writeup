  doi = {10.1007/978-1-4419-9389-2_9}
}

@article{waszczuk2018,
  title = {Redefining {{Phenotypes}} to {{Advance Psychiatric Genetics}}: {{Implications}} from {{Hierarchical Taxonomy}} of {{Psychopathology}}},
  url = {https://psyarxiv.com/sf46g/},
  doi = {10.31234/osf.io/sf46g},
  shorttitle = {Redefining {{Phenotypes}} to {{Advance Psychiatric Genetics}}},
  abstract = {Genetic discovery in psychiatry is hindered by suboptimal phenotypic definitions. We argue that the hierarchical, dimensional, and data-driven classification system proposed by the Hierarchical Taxonomy of Psychopathology (HiTOP) consortium provides a more effective approach to identifying genes that underlie mental disorders, and studying psychiatric etiology, than current diagnostic categories. Specifically, genes are expected to operate at different levels of the HiTOP hierarchy, with some highly pleiotropic genes influencing higher-order psychopathology (e.g. the general factor), whereas other genes confer specific risk for individual spectra (e.g. internalizing), subfactors (e.g. fear disorders), or narrow symptoms (e.g. mood instability). We demonstrate that the HiTOP model aligns well with the current understanding of the higher-order genetic structure of psychopathology that emerged from a large body of family and twin studies. We discuss the convergence between the HiTOP model and findings from recent molecular studies of psychopathology indicating broad genetic pleiotropy, such as cross-disorder SNP-based genetic correlations and polygenic risk scores, and we highlight molecular genetic studies that have successfully redefined phenotypes to enhance precision and statistical power. Finally, we suggest how to integrate the HiTOP approach into future molecular genetic research, including quantitative and hierarchical assessment tools for future data-collection, and tips for phenotypic analyses.},
  journaltitle = {PsyArXiv},
  urldate = {2018-08-28},
  date = {2018-08-28},
  author = {Waszczuk, Monika},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GT9L6SUN/Waszczuk - 2018 - Redefining Phenotypes to Advance Psychiatric Genet.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/IJM7YEL9/sf46g.html}
}

@article{nishi2015,
  langid = {english},
  title = {Inequality and Visibility of Wealth in Experimental Social Networks},
  volume = {526},
  issn = {1476-4687},
  url = {https://www.nature.com/articles/nature15392},
  doi = {10.1038/nature15392},
  abstract = {Humans prefer relatively equal distributions of resources1,2,3,4,5, yet societies have varying degrees of economic inequality6. To investigate some of the possible determinants and consequences of inequality, here we perform experiments involving a networked public goods game in which subjects interact and gain or lose wealth. Subjects (n = 1,462) were randomly assigned to have higher or lower initial endowments, and were embedded within social networks with three levels of economic inequality (Gini coefficient = 0.0, 0.2, and 0.4). In addition, we manipulated the visibility of the wealth of network neighbours. We show that wealth visibility facilitates the downstream consequences of initial inequality—in initially more unequal situations, wealth visibility leads to greater inequality than when wealth is invisible. This result reflects a heterogeneous response to visibility in richer versus poorer subjects. We also find that making wealth visible has adverse welfare consequences, yielding lower levels of overall cooperation, inter-connectedness, and wealth. High initial levels of economic inequality alone, however, have relatively few deleterious welfare effects.},
  number = {7573},
  journaltitle = {Nature},
  urldate = {2018-08-28},
  date = {2015-10},
