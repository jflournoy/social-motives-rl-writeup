  keywords = {5-HTT,Memory,emotion,polymorphism,retrograde amnesia,serotonin (5-HT)},
  author = {Strange, Bryan A. and Kroes, Marijn C. W. and Roiser, Jonathan P. and Tan, Geoffrey C. Y. and Dolan, Raymond J.},
