  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4CAH8EX9/Reynolds et al. - 2013 - Analogue Study of Peer Influence on Risk-Taking Be.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/X4IPVZXR/10.html}
