  shortjournal = {Pediatrics},
  urldate = {2014-10-08},
  date = {2014-10-06},
  pages = {peds.2014-1974},
  keywords = {Sexual behavior,adolescents,teen sexting},
  author = {Temple, Jeff R. and Choi, HyeJeong},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/J73I6PUD/Temple and Choi - 2014 - Longitudinal Association Between Teen Sexting and .pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/NSVD3T5A/peds.2014-1974.html},
  eprinttype = {pmid},
