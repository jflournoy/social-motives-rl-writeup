@article{DishionAdolescentFriendshipDynamic2004,
  langid = {english},
  title = {Adolescent {{Friendship}} as a {{Dynamic System}}: {{Entropy}} and {{Deviance}} in the {{Etiology}} and {{Course}} of {{Male Antisocial Behavior}}},
  volume = {32},
  issn = {0091-0627, 1573-2835},
  url = {http://link.springer.com/article/10.1023/B%3AJACP.0000047213.31812.21},
  doi = {10.1023/B:JACP.0000047213.31812.21},
  shorttitle = {Adolescent {{Friendship}} as a {{Dynamic System}}},
  abstract = {A dynamic systems framework was applied to understand the influence of friendship on antisocial behavior from childhood (age 9–10) through adulthood (age 24–25) for Oregon Youth Study males (N = 206). Boys were videotaped interacting with a friend at ages 14, 16, and 18, and deviant content and interpersonal processes were independently coded. Conditional dyadic interpersonal processes were studied as a communication system and summarized by an index of information entropy (F. Attneave, 1959). High entropy scores represent disorganized, unpredictable patterns of interaction, whereas low entropy scores reflect an organized dialogue. Conversations of early-onset antisocial boys and their best friends were less organized and included more deviant content than those of well-adjusted controls. Prediction analyses, however, revealed an interaction between entropy and deviant talk. Consistent with expectation, males with well-organized interactions (i.e., low entropy) but elevated levels of deviant content were most likely to continue antisocial behavior into adulthood. Findings suggest that individual risk for maladaptation may be amplified by early adolescent friendship dynamics organized around deviance.},
  number = {6},
  journaltitle = {Journal of Abnormal Child Psychology},
  shortjournal = {J Abnorm Child Psychol},
  urldate = {2013-02-25},
  date = {2004-12-01},
  pages = {651-663},
  keywords = {Clinical Psychology,Developmental Psychology,antisocial behavior,dyadic interaction,friendship,peer relations},
  author = {Dishion, Thomas J. and Nelson, Sarah E. and Winter, Charlotte E. and Bullock, Bernadette Marie},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/85SIGQ29/Dishion et al. - 2004 - Adolescent Friendship as a Dynamic System Entropy.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/T4WKN9U9/10.1023BJACP.0000047213.31812.html}
}

