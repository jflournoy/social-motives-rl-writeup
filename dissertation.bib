@article{JayawickremeVirtuousstatesvirtuous2014,
  langid = {english},
  title = {Virtuous States and Virtuous Traits: {{How}} the Empirical Evidence Regarding the Existance of Broad Traits Saves Virtue Ethics from the Situationist Critique},
  issn = {1477-8785, 1741-3192},
  url = {http://tre.sagepub.com/content/early/2014/08/05/1477878514545206},
  doi = {10.1177/1477878514545206},
  shorttitle = {Virtuous States and Virtuous Traits},
  abstract = {A major objection to the study of virtue asserts that the empirical psychological evidence implies traits have little meaningful impact on behavior, as slight changes in situational characteristics appear to lead to large changes in virtuous behavior. We argue in response that the critical evidence is not these effects of situations observed in social psychological experiments, but evidence of stable individual differences obtained from correlations of individual’s behaviors across multiple contexts. The totality of the empirical evidence is shown to support this claim: broad traits are real, prominent, and consequential, and these traits, conceptualized as density distributions of personality states, exhibit remarkable consistency. In short, the evidence in favor of individual differences is empirically solid, and the study of ethics focused on virtue is not threatened by psychological research.},
  journaltitle = {Theory and Research in Education},
  shortjournal = {Theory and Research in Education},
  urldate = {2015-05-15},
  date = {2014-08-06},
  pages = {1477878514545206},
  keywords = {traits,density distributions of personality states,situationism,virtue theory},
  author = {Jayawickreme, Eranda and Meindl, Peter and Helzer, Erik G. and Furr, R. Michael and Fleeson, William},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/F33U39FG/Jayawickreme et al. - 2014 - Virtuous states and virtuous traits How the empir.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/R6IUDERG/Jayawickreme et al. - 2014 - Virtuous states and virtuous traits How the empir.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FJTQ75JK/1477878514545206.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GGFERASA/1477878514545206.html}
}

@article{crone2012,
  langid = {english},
  title = {Understanding Adolescence as a Period of Social–Affective Engagement and Goal Flexibility},
  volume = {13},
  issn = {1471-003X},
  doi = {10.1038/nrn3313},
  abstract = {Research has demonstrated that extensive structural and functional brain development continues throughout adolescence. A popular notion emerging from this work states that a relative immaturity in frontal cortical neural systems could explain adolescents' high rates of risk-taking, substance use and other dangerous behaviours. However, developmental neuroimaging studies do not support a simple model of frontal cortical immaturity. Rather, growing evidence points to the importance of changes in social and affective processing, which begin around the onset of puberty, as crucial to understanding these adolescent vulnerabilities. These changes in social–affective processing also may confer some adaptive advantages, such as greater flexibility in adjusting one's intrinsic motivations and goal priorities amidst changing social contexts in adolescence.},
  number = {9},
  journaltitle = {Nature Reviews Neuroscience},
  shortjournal = {Nat Rev Neurosci},
  date = {2012-09},
  pages = {636-650},
  keywords = {_tablet},
  author = {Crone, E. A. and Dahl, Ronald E.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/9S5U6VFX/Crone_Dahl-2012-Understanding_adolescence_as_a_period_of_social–affective_engagement_and_goal.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CJK25IMU/Crone_Dahl-2012-Understanding_adolescence_as_a_period_of_social–affective_engagement_and_goal.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/I3B47D9M/nrn3313.html}
}

@article{bos_what_2009,
  langid = {english},
  title = {What Motivates Repayment? {{Neural}} Correlates of Reciprocity in the {{Trust Game}}},
  issn = {1749-5016, 1749-5024},
  url = {http://scan.oxfordjournals.org/content/early/2009/03/20/scan.nsp009},
  doi = {10.1093/scan/nsp009},
  shorttitle = {What Motivates Repayment?},
  abstract = {Reciprocity of trust is important for social interaction and depends on individual differences in social value orientation (SVO). Here, we examined the neural correlates of reciprocity by manipulating two factors that influence reciprocal behavior: (1) the risk that the trustor took when trusting and (2) the benefit for the trustee when being trusted. FMRI results showed that anterior Medial Prefrontal Frontal Cortex (aMPFC) was more active when participants defected relative to when participants reciprocated, but was not sensitive to manipulations of risk and benefit or individual differences in SVO. However, activation in the temporal-parietal-junction (rTPJ), bilateral anterior insula and anterior cingulate cortex (ACC) was modulated by individual differences in SVO. In addition, these regions were differentially sensitive to manipulations of risk for the trustor when reciprocating. In contrast, the ACC and the right dorsolateral prefrontal cortex were sensitive to the benefit for the trustee when reciprocating. Together, the results of this study provide more insight in how several brain regions work together when individuals reciprocate trust, by showing how these regions are differentially sensitive to reciprocity motives and perspective-taking.},
  journaltitle = {Social Cognitive and Affective Neuroscience},
  shortjournal = {Soc Cogn Affect Neurosci},
  urldate = {2015-10-03},
  date = {2009-03-20},
  pages = {nsp009},
  keywords = {Decision-making,Social cognition,fMRI,reciprocity,trust game},
  author = {van den Bos, Wouter and van Dijk, Eric and Westenberg, Michiel and Rombouts, Serge A. R. B. and Crone, Eveline A.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/BJCEWFM9/Bos et al. - 2009 - What motivates repayment Neural correlates of rec.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/HI2SIVND/scan.nsp009.html},
  eprinttype = {pmid},
  eprint = {19304843}
}

@article{WiltDynamicRolePersonality2012,
  langid = {english},
  title = {The {{Dynamic Role}} of {{Personality States}} in {{Mediating}} the {{Relationship Between Extraversion}} and {{Positive Affect}}},
  volume = {80},
  issn = {1467-6494},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1467-6494.2011.00756.x/abstract},
  doi = {10.1111/j.1467-6494.2011.00756.x},
  abstract = {One of the most noteworthy and robust findings in personality psychology is the relationship between extraversion and positive affect. Existing theories have debated the origins and nature of this relationship, offering both structural/fixed and environmental/dynamic explanations. We tested the novel and straightforward dynamic hypothesis that part of the reason trait extraversion predicts trait positive affect is through an increased propensity to enact extraverted states, which in turn leads to experiencing more positive affect states. We report 5 experience sampling studies (and a meta-analysis of primary studies) conducted in natural environments and laboratory settings in which undergraduate participants (N = 241) provided ratings of trait extraversion, trait positive affect, extraversion states, and positive affect states. Results of primary studies and the meta-analysis showed that relationships between trait extraversion and trait positive affect were partially mediated by aggregated extraversion states and aggregated positive affect states. The results supported our dynamic hypothesis and suggested that dynamic explanations of the relationship between trait extraversion and trait positive affect are compatible with structural explanations. An important implication of these findings is that individuals might be able to increase their happiness by self-regulating their extraverted states.},
  number = {5},
  journaltitle = {Journal of Personality},
  shortjournal = {J Pers},
  urldate = {2015-05-15},
  date = {2012-10-01},
  pages = {1205-1236},
  author = {Wilt, Joshua and Noftle, Erik E. and Fleeson, William and Spain, Jana S.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/75WSAE7C/Wilt et al. - 2012 - The Dynamic Role of Personality States in Mediatin.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/MBSFN45U/Wilt et al. - 2012 - The Dynamic Role of Personality States in Mediatin.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CA3B7WUI/full.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CW2GDPZ2/full.html}
}

