  author = {Landis, Drew and Courtney, William and Dieringer, Christopher and Kelly, Ross and King, Margaret and Miller, Brittny and Wang, Runtang and Wood, Dylan and Turner, Jessica A. and Calhoun, Vince D.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/K8GUTKU8/Landis et al. - 2016 - COINS Data Exchange An open platform for compilin.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CDN9XQ8R/S1053811915004310.html}
