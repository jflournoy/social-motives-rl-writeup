  keywords = {ANTIDEPRESSANTS,HPA system,Psychopharmacology,Remission,Response,depression},
  author = {Scharnholz, Barbara and Weber-Hamann, Bettina and Lederbogen, Florian and Schilling, Claudia and Gilles, Maria and Onken, Vera and Frankhauser, Pascal and Kopf, Daniel and Deuschle, Michael},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/V5T2J636/Scharnholz et al. - 2010 - Antidepressant treatment with mirtazapine, but not.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/DDEIKZXS/S0165178109003163.html}
