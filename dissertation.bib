  author = {Galvan, Adriana and Hare, Todd A. and Parra, Cindy E. and Penn, Jackie and Voss, Henning and Glover, Gary and Casey, B. J.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/M6W3DEXR/Galvan et al. - 2006 - Earlier development of the accumbens relative to o.pdf},
