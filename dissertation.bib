  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/RB3PDVEF/Kuster and Orth - 2013 - The Long-Term Stability of Self-Esteem Its Time-De.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ZT8S6R7X/677.html},
  eprinttype = {pmid},
