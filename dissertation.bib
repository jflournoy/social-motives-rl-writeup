  pages = {873-879},
  keywords = {Health,Interpersonal Relations,Trauma,Family,intragroup processes},
  author = {Stroebe, Margaret and Schut, Henk},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/VAQAWKVS/Stroebe and Schut - 2015 - Family Matters in Bereavement Toward an Integrativ.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/TW5228AB/873.html},
  eprinttype = {pmid},
  eprint = {26581741}
}

@article{HershfieldLeveragingPsychologicalInsights2015,
