  author = {Taquet, Maxime and Quoidbach, Jordi and de Montjoye, Yves-Alexandre and Desseilles, Martin and Gross, James J.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/U7JR3R7G/Taquet_et_al-2016-Hedonism_and_the_choice_of_everyday_activities.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/33JBR6EJ/9769.abstract.html},
