@article{LevineAssessingDeceptionDetection2010,
  title = {Assessing {{Deception Detection Accuracy}} with {{Dichotomous Truth}}–{{Lie Judgments}} and {{Continuous Scaling}}: {{Are People Really More Accurate When Honesty Is Scaled}}?},
