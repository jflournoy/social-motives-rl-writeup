  author = {Whiteside, Stephen P. and Lynam, Donald R. and Miller, Joshua D. and Reynolds, Sarah K.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/DYS6U7A7/Whiteside et al. - 2005 - Validation of the UPPS impulsive behaviour scale .pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JAEEGFRC/TDS_UPPS-P.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/9DXM8PAZ/abstract.html}
