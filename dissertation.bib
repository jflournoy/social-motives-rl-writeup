  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4BXNSHEF/Fleeson et al. - 2014 - Character The Prospects for a Personality-Based P.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E7I57B6G/Fleeson et al. - 2014 - Character The Prospects for a Personality-Based P.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/7AT7FI82/full.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/IG7FMCNX/abstract.html}
