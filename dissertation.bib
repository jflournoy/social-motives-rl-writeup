  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/6IFP4DZJ/Hawk - 2013 - Examining the Interpersonal Reactivity Index (IRI).pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WNB7WASW/Hawk et al. - Examining the Interpersonal Reactivity Index (IRI).pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/XNM48KEM/Hawk et al. - 2013 - Examining the Interpersonal Reactivity Index (IRI).pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/225FSM64/00223891.2012.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/D6ZC257W/00223891.2012.html}
