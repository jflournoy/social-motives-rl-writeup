  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/3A4RXC4J/Bowers_2014_PYD.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WIEQ9MIE/Bowers et al. - 2014 - Special Issue Introduction Thriving Across the Ad.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ZGD4WJIQ/Bowers_2014_PYD.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CU6IV4IG/s10964-014-0117-8.html}
