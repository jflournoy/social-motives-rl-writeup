  author = {Konen, Christina S. and Kastner, Sabine},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/VJMR62X4/Konen and Kastner - 2008 - Two hierarchically organized neural systems for ob.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/N6TU56D9/nn2036.html}
