  urldate = {2014-05-09},
  date = {2012-10-15},
  pages = {600-610},
  keywords = {Figurative,Inferior frontal gyrus,Lateralisation,Metaphor,Middle temporal gyrus,Motor,Pragmatic,Right hemisphere,Semantic,Sentence},
  author = {Rapp, Alexander M. and Mutschler, Dorothee E. and Erb, Michael},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4T9T4V3T/Rapp et al. - 2012 - Where in the brain is nonliteral language A coord.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4EGUFFJ8/S1053811912006362.html}
