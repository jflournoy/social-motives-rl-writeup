@article{katz_effects_2000,
  langid = {english},
  title = {Effects of {{Outcome Expectancies}} and {{Personality}} on {{Young Adults}}' {{Illicit Drug Use}}, {{Heavy Drinking}}, and {{Risky Sexual Behavior}}},
  volume = {24},
