  journaltitle = {Child Development},
  urldate = {2013-12-02},
  date = {2009},
  pages = {1016--1038},
  author = {Pfeifer, Jennifer H. and Masten, Carrie L. and Borofsky, Larissa A. and Dapretto, Mirella and Fuligni, Andrew J. and Lieberman, Matthew D.},
