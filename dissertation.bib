  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/9B5RXS7J/Acerbi et al. - 2013 - The Expression of Emotions in 20th Century Books.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/MPDFGHCA/Acerbi et al. - 2013 - The Expression of Emotions in 20th Century Books.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/43QWVAU3/infodoi10.1371journal.pone.html}
