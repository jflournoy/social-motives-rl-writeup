@article{Personalitytraitsshare2018,
  langid = {english},
  title = {Personality Traits Share Overlapping Neuroanatomical Correlates with Internalizing and Externalizing Psychopathology},
  url = {https://osf.io/sjhx3/},
  doi = {None},
  abstract = {Although personality traits have been linked to internalizing and externalizing psychopathology,
the extent to which these traits and psychopathological phenotypes share a common
neuroanatomical structure is unclear. To address this gap, we used structural neuroimaging and
self-report data from 1104 participants in the Human Connectome Project to characterize the
neuroanatomical correlates of Five Factor Model (FFM) traits. We generated neuroanatomical
profiles (NAPs) of FFM traits and psychopathology indices based on the thickness and surface
area of each cortical region in the Desikan atlas. We used a profile matching approach to
compare the absolute similarity of the FFM trait NAPs and psychopathology index NAPs.
Evaluating the individual regions in the NAPs suggest that FFM traits show small associations
with thickness and surface area in brain regions throughout the cortex, with prefrontal regions
being the most strongly related to multiple personality traits. Profile matching analyses indicated
that the NAPs derived from Neuroticism and Extraversion demonstrated large absolute
similarities to the NAPs of internalizing psychopathology. Similarly, the NAPs of Agreeableness
and Conscientiousness showed medium to large relations with the NAPs of externalizing
psychopathology. These results suggest that similar neuroanatomical correlates underlie specific
personality traits and symptoms of psychopathology, providing support for dimensional models
that incorporate personality traits into the etiology and manifestation of psychopathology. 
    Hosted on the Open Science Framework},
  journaltitle = {Open Science Framework},
  urldate = {2018-03-01},
  date = {2018-02-16},
  keywords = {_reading,HiTOP},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/YBGKE67W/2018-Personality_traits_share_overlapping_neuroanatomical_correlates_with.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/587PXWXM/z25yg.html}
}

@article{mendle2018,
  langid = {english},
  title = {Age at {{Menarche}}, {{Depression}}, and {{Antisocial Behavior}} in {{Adulthood}}},
  volume = {141},
  issn = {0031-4005, 1098-4275},
  url = {http://pediatrics.aappublications.org/content/141/1/e20171703},
  doi = {10.1542/peds.2017-1703},
  abstract = {BACKGROUND: Early pubertal timing in girls is one of the best-replicated antecedents of a range of mental health problems during adolescence, but few researchers have examined the duration of these effects.
METHODS: We leverage a nationally representative sample (N = 7802 women) managed prospectively from adolescence over a period of ∼14 years to examine associations of age at menarche with depressive symptoms and antisocial behaviors in adulthood.
RESULTS: Earlier ages at menarche were associated with higher rates of both depressive symptoms and antisocial behaviors in early-middle adulthood largely because difficulties that started in adolescence did not attenuate over time.
CONCLUSIONS: These findings indicate that the emotional sequelae of puberty extend further than documented in previous research, and suggest that earlier development may place girls on a life path from which it may be difficult to deviate. The American Academy of Pediatrics already provides guidelines for identifying and working with patients with early pubertal timing. Pediatricians and adolescent health care providers should also be attuned to early maturers’ elevated mental health risk and sensitive to the potential duration of changes in mental health that begin at puberty.},
