  author = {de Vries, Reinout E.},
  options = {useprefix=true},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/B4XDVR8R/de Vries - 2010 - Lots of target variance An update of SRM using th.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/UE9B7JT7/de Vries - 2010 - Lots of target variance An update of SRM using th.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GGNQUQCQ/abstract.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/TPDX62CG/abstract.html}
