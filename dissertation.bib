  eprinttype = {pmid},
  eprint = {22816531}
}

@article{landsverk_bridging_2011,
  langid = {english},
  title = {Bridging Science and Practice in Child Welfare and Children’s Mental Health Service Systems through a Two-Decade Research Center Trajectory},
  volume = {11},
